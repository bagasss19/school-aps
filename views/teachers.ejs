<!DOCTYPE html>
<html>
  <%- include ("./partials/header.ejs", {title : "Teacher List"}) %>
    <body>
        <table style="border: 1px;">
            <thead>
              <th>Id</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Email</th>
              <th>Gender</th>
            </thead>
            <% data.forEach(x => { %>
            <tbody>
              <td><%= x.id %></td>
              <td><%= x.first_name %></td>
              <td><%= x.last_name %></td>
              <td><%= x.email %></td>
              <td><%= x.gender %></td>
            </tbody>
            <% }) %>
          </table>
    </body>
</html>